<!DOCTYPE html>
<!--[if IE]><![endif]-->
<!--[if IE 8 ]><html dir="ltr" lang="en" class="ie8"><![endif]-->
<!--[if IE 9 ]><html dir="ltr" lang="en" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="ltr" lang="en">
  <!--<![endif]-->
  <head>
    <!-- Basic page needs ================================================== -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    {% if settings.favicon_enable %}
    <link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}" />
    <link rel="apple-touch-icon" href="{{ 'favicon.png' | asset_url }}" />
    {% endif %}
    <!-- Title and description ================================================== -->
    <title>
      {{ page_title }}{% if current_tags %}{% assign current_tags_joined = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: current_tags_joined }}{% endif %}{% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
    </title>
    {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}">
    {% endif %}
    <base href="{{ canonical_url }}" />

    <!-- Product meta ================================================== -->
    {% include 'open-graph-tags' %}
    {% include 'twitter-card' %}

    <!-- Helpers ================================================== -->
    <link rel="canonical" href="{{ canonical_url }}">

    <!-- CSS ================================================== -->
    {{ 'owl.carousel.min.css' | asset_url | stylesheet_tag }}
    {{ 'owl.theme.green.min.css' | asset_url | stylesheet_tag }}
    {{ 'menu.css' | asset_url | stylesheet_tag }}
    {{ 'animate.css' | asset_url | stylesheet_tag }}
    {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }} 
    {{ '//use.fontawesome.com/releases/v5.6.1/css/all.css' | stylesheet_tag }}
    {{ '//maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css' | stylesheet_tag }}
    {{ '//cdn.jsdelivr.net/npm/pixeden-stroke-7-icon@1.2.3/pe-icon-7-stroke/dist/pe-icon-7-stroke.min.css' | stylesheet_tag }}
    {{ 'pe-helper.css' | asset_url | stylesheet_tag }}
    {{ 'bebas_neue.css' | asset_url | stylesheet_tag }}
    {% comment %}{{ 'simple-line-icons.css' | asset_url | stylesheet_tag }}{% endcomment %}
    {{ '//cdnjs.cloudflare.com/ajax/libs/simple-line-icons/2.4.1/css/simple-line-icons.min.css' | stylesheet_tag }}
    {{ 'stroke-gap-icons.css' | asset_url | stylesheet_tag }}
    {% include 'google-fonts' %}
    {% if template == 'index' %}{{ 'slideshow.css' | asset_url | stylesheet_tag }}{% endif %}
    {{ 'stylesheet.css' | asset_url | stylesheet_tag }}
    {{ 'ajaxlogin.css' | asset_url | stylesheet_tag }}
    {{ 'quickview.css' | asset_url | stylesheet_tag }}
    {{ 'magnific-popup.css' | asset_url | stylesheet_tag }}
    {% if template contains 'product' %}
    {{ 'zoom.css' | asset_url | stylesheet_tag }}    
    {{ 'cloud-zoom.css' | asset_url | stylesheet_tag }}    
    {% endif %}
    {{ 'category.css' | asset_url | stylesheet_tag }}    
    {% if template contains 'collection' or template contains 'blog' or template contains 'article' %}
    {{ 'filter.css' | asset_url | stylesheet_tag }}    
    {% endif %}
    {{ 'custom.css' | asset_url | stylesheet_tag }}
    {{ 'https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.3/toastr.min.css' | stylesheet_tag }}
    {% if settings.lazyloading_enable -%}
    {{ 'https://cdnjs.cloudflare.com/ajax/libs/lazysizes/5.1.2/lazysizes.min.js' | script_tag }}
    {% endif %}
    {% if template == 'page.faq' %}
    {{ 'faq-style.css' | asset_url | stylesheet_tag }}    
    {% endif %}

    <!-- Header hook for plugins ================================================== -->
    {{ content_for_header }}
    {% include 'oldIE-js' %}
    <!-- JavaScripts-->
    {{ 'jquery-2.1.1.min.js' | asset_url | script_tag }}
    {{ 'jquery-ui.min.js' | asset_url | script_tag }}
    {{ 'bootstrap.min.js' | asset_url | script_tag }}
    {{ 'owl.carousel.min.js' | asset_url | script_tag }} 
    {{ 'jquery.plugin.min.js' | asset_url | script_tag }}
    {{ 'jquery.countdown.min.js' | asset_url | script_tag }}
    {{ 'menu.js' | asset_url | script_tag }}
    {% if settings.grid_color_swatches %}{{ 'swatches.js' | asset_url | script_tag }}{% endif %}
    {% if template contains 'collection' and template != 'list-collections' %}
    {{ 'grid.js' | asset_url | script_tag }}
    {% endif %}
    {% if template contains 'collection' %}
    {% if template != 'list-collections' %}
    {{ 'filter.js' | asset_url | script_tag }}    
    {% endif %}
    {% endif %}
    {% if template == 'index' %}
    {{ 'jquery.nivo.slider.js' | asset_url | script_tag }}
    {{ '//unpkg.com/spritespin@x.x.x/release/spritespin.js' | script_tag }}
    {% endif %}
    {{ 'moment.min.js' | asset_url | script_tag }}
    {{ 'jquery.bpopup.min.js' | asset_url | script_tag }}
    {{ 'jquery.cookie.js' | asset_url | script_tag }}
    {{ 'quickview.js' | asset_url | script_tag }}
    {{ 'common.js' | asset_url | script_tag }}
    {{ 'jquery.magnific-popup.min.js' | asset_url | script_tag }}
    {{ 'https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.3/toastr.min.js' | script_tag }}
    {{ 'shipping-cart.js' | asset_url | script_tag }}
    {% if template != 'cart' %}
    {{ 'option_selection.js' | shopify_asset_url | script_tag }}
    {% endif %}
    {% if template contains 'product' %}
    {{ 'cloud-zoom.1.0.2.min.js' | asset_url | script_tag }}
    {{ 'zoom.js' | asset_url | script_tag }}  
    {% endif %}
    {% if template == 'page.faq' %}
    {{ 'faq-main.js' | asset_url | script_tag }}
    {% endif %}
    {% if template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
    {% endif %}
  </head>
  <body class="{% case template %}
               {% when 'index' %}
               common-home
               {% when 'collection' %}
               collection
               {% when 'product' %}
               product-product
               {% when 'blog' %}
               blog-blog
               {% when 'article' %}
               blog-article
               {% when 'cart' %}
               checkout-cart
               {% when 'customers/login' %}
               account-login
               {% else %} {{ template }}
               {% endcase %} home1" >
    <!--[if lt IE 8]>
<p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
<![endif]-->
    <div class="wrapper">
      {% section 'header' %}
      {% if template == 'index' %}
      <div id="content">
        {% endif %}
        {{ content_for_layout }}
        {% if template == 'index' %}
      </div>
      {% endif %}
      <!-- End Content -->  

      {% include 'quickview' %}

      {% section 'footer' %}

      {% if settings.show_pupup and template == 'index'%}
      {% include 'popup' %}
      {% endif %}

      <script>
        var moneyFormat = '{{ shop.money_format }}';
      </script>

      {% if template contains 'product' %}
      {{ 'handlebars.min.js' | asset_url | script_tag }}
      {% endif %}
      {% if settings.ajax_cart_method != 'page' %}
      {% include 'ajax-cart-template' %}
      {{ 'ajaxify.js' | asset_url | script_tag }}

      <script>
        jQuery(function($) {
          ajaxifyShopify.init({
            method: '{{ settings.ajax_cart_method }}',
            wrapperClass: 'product-detail',
            formSelector: '#addToCartForm',
            addToCartSelector: '#addToCart',
            cartCountSelector: '.cart-count',
            cartCostSelector: '.cart-cost',
            toggleCartButton: '.cart-toggle',
            useCartTemplate: true,
            btnClass: 'btn',
            moneyFormat: {{ shop.money_format | json }},
            disableAjaxCart: false,
            enableQtySelectors: true
          });
        });
      </script>
      {% endif %}
      {{ 'fastclick.min.js' | asset_url | script_tag }}
      {% if template  contains 'product' %}
      {{ 'shop.js' | asset_url | script_tag }}
      {% endif %}
      {% if settings.hide_wishlist == false %}
      {{ 'Wishlist.js' | asset_url | script_tag }}
      <script>
        (function() {
          Wishlist.init();
        }());
      </script>
      {% endif %}
      {% if settings.search_autofil_enable %}{% include 'search-autocomplete' %}{% endif %}
      {% if settings.show_gdpr_msg %}{% include 'cookie-consent' %} {% endif %}
      {% include 'currencies' %}
    </div>
  </body>
</html>